<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bs3:grid
        xsi:schemaLocation="http://causeway.apache.org/applib/layout/component http://causeway.apache.org/applib/layout/component/component.xsd   http://causeway.apache.org/applib/layout/grid/bootstrap3 http://causeway.apache.org/applib/layout/grid/bootstrap3/bootstrap3.xsd"
        xmlns:bs3="http://causeway.apache.org/applib/layout/grid/bootstrap3"
        xmlns:cpt="http://causeway.apache.org/applib/layout/component"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bs3:row>
        <bs3:col span="12" unreferencedActions="true">
            <cpt:domainObject bookmarking="AS_ROOT"/>
            <cpt:action bookmarking="NEVER" cssClassFa="fa fa-fw fa-circle" cssClassFaPosition="LEFT" id="clearHints">
                <cpt:named>Clear Hints</cpt:named>
            </cpt:action>
        </bs3:col>
    </bs3:row>
    <bs3:row>
        <bs3:col span="12">
            <bs3:row>
                <bs3:col span="12">
                    <bs3:tabGroup>
                        <bs3:tab name="Diagram">
                            <bs3:row>
                                <bs3:col span="12">
                                    <cpt:fieldSet name="Diagram" id="other" unreferencedProperties="true">
                                        <cpt:property id="diagram" labelPosition="NONE" typicalLength="400">>
                                            <cpt:named>Diagram</cpt:named>
                                        </cpt:property>
                                    </cpt:fieldSet>
                                </bs3:col>
                            </bs3:row>
                        </bs3:tab>
                        <bs3:tab name="Other">
                            <bs3:row>
                                <bs3:col span="12">
                                    <cpt:fieldSet name="Other" id="other" unreferencedProperties="true">
                                        <cpt:property id="document" labelPosition="NOT_SPECIFIED">
                                            <cpt:named>Document</cpt:named>
                                        </cpt:property>
                                        <cpt:property id="id" labelPosition="NOT_SPECIFIED" typicalLength="18">
                                            <cpt:named>Id</cpt:named>
                                        </cpt:property>
                                        <cpt:property id="text" labelPosition="NOT_SPECIFIED" typicalLength="25">
                                            <cpt:named>Text</cpt:named>
                                        </cpt:property>
                                        <cpt:property id="typedDependencies" labelPosition="NOT_SPECIFIED"
                                                      typicalLength="25">
                                            <cpt:named>Typed Dependencies</cpt:named>
                                        </cpt:property>
                                        <cpt:property id="version" labelPosition="NOT_SPECIFIED" typicalLength="9">
                                            <cpt:named>Version</cpt:named>
                                        </cpt:property>
                                    </cpt:fieldSet>
                                </bs3:col>
                            </bs3:row>
                        </bs3:tab>
                        <bs3:tab name="Metadata">
                            <bs3:row>
                                <bs3:col span="12">
                                    <cpt:fieldSet name="Metadata" id="metadata">
                                        <cpt:action id="downloadLayoutXml" position="PANEL_DROPDOWN"/>
                                        <cpt:action id="downloadMetamodelXml" position="PANEL_DROPDOWN"/>
                                        <cpt:action id="inspectMetamodel" position="PANEL_DROPDOWN"/>
                                        <cpt:action id="openRestApi" position="PANEL_DROPDOWN"/>
                                        <cpt:action id="rebuildMetamodel" position="PANEL_DROPDOWN"/>
                                        <cpt:property hidden="ALL_TABLES" id="objectType" typicalLength="25">
                                            <cpt:named>Object Type</cpt:named>
                                        </cpt:property>
                                        <cpt:property hidden="ALL_TABLES" id="objectIdentifier" typicalLength="25">
                                            <cpt:named>Object Identifier</cpt:named>
                                        </cpt:property>
                                    </cpt:fieldSet>
                                </bs3:col>
                            </bs3:row>
                        </bs3:tab>
                    </bs3:tabGroup>
                </bs3:col>
            </bs3:row>
            <bs3:row>
                <bs3:col span="12">
                    <cpt:fieldSet name="Details" id="details"/>
                </bs3:col>
            </bs3:row>
        </bs3:col>
    </bs3:row>
</bs3:grid>
